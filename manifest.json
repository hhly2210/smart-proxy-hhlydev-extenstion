{
    "manifest_version": 3,
    "name": "Smart Proxy Switcher",
    "version": "1.0",
    "description": "A smart proxy management extension",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "permissions": [
        "storage",
        "proxy",
        "tabs",
        "webRequest",
        "scripting",
        "declarativeNetRequest",
        "declarativeNetRequestWithHostAccess",
        "notifications",
        "privacy"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "options_page": "options.html"
}